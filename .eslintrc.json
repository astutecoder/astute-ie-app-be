{
  "extends": ["prettier"],
  "plugins": ["prettier"],
  "rules": {
    "quotes": ["error", "single"],
    "prettier/prettier": [
      "error",
      {
        "printWidth": 120
      }
    ],
    "sort-imports": "error",
    "spaced-comment": "warn",
    "space-in-parens": "warn",
    "no-trailing-spaces": "warn",
    "no-empty-function": "error"
  }
}
